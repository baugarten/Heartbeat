define([
    'jquery',
    'underscore',
    'backbone',
    'router'
], function($, _, Backbone, Router) {
    var load = function(url, callback) {
        $.ajax({
            url: url,
            success: function(resp, b, c) {
                if (resp['redirect']) {
                    load(resp['redirect']);
                }
                callback(resp, b, c);
            }
        });
    }

    var initialize = function() {
        Router.initialize();
    }
    return {
        initialize: initialize
    };
});